button{background: none;margin:0;padding:0;}
button::after{border:none;}
.button-hover {background:none;}


.weapbox{overflow: hidden;margin:20rpx 15rpx 0;}
button.speed_btn{
  display: inline-block;
  width: 60rpx;
  height: 60rpx;
  padding: 0 1rpx; 
  line-height: 60rpx;
  font-size: 28rpx;
  font-weight: bold;
  font-family: "Microsoft YaHei"!important;
  margin: 0 4rpx 10rpx; 
  border-radius: 10rpx;
  border: 2rpx solid #62321c;
  background-color: #FFFFFF;
  color: #65371d!important;
  text-align: center;
}
.disable{border: 2rpx solid #62321c;background-color: #d9d9d9;}
.noend view{display: inline-block;}
