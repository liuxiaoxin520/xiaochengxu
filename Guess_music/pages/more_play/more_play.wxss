.allbg{
  background: #000438;
  width: 100%;
  height: 100%;
}
.theme_list{
  height: 54px;
  margin: 5px 20px;
  padding: 10px 10px;
  border-radius: 8px;
  background-color:#cd5be9;
  opacity: 0.9;
}
.theme_img{
  width: 54px;
  height: 54px;
  margin-right: 15px;
  border-radius: 50%;
  background-color: red;
  float: left;
}
.theme_name{
  color: white;
  font-size: 16px;
  line-height: 54px;
  float: left;
}
.jump_icon{
  margin-top: 7px;
  width: 40px;
  height: 40px;
  float: left;
}
.go{
  line-height: 54px;
  color: white;
  font-size:14px;
  float: left;
}
.go_box{
  float: right;
}
