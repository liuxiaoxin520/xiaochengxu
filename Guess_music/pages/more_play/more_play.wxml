<view class='allbg'>
  <view wx:for="{{items}}" class="theme_list">
    <image src='{{item.img}}' class="theme_img"></image>
    <view class='theme_name' >{{item.name}}</view>
    <view class='go_box' bindtap='more_play' data-appid="{{item.appid}}">
      <view class='go'>GO >></view>
      <image class='jump_icon' src='../../style/images/run.gif'></image>
    </view>
  </view>
</view>