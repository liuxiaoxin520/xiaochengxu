<!--MyRecord.wxml-->
<view class="header">
  <view class="header_left {{addClass?'current':''}}" bindtap='header_left'>节日主题</view>
  <view class="header_center {{addClass1?'current':''}}" bindtap='header_center'>生日主题</view>
  <view class="header_right {{addClass2?'current':''}}" bindtap='header_right'>婚礼主题</view>
  <view class='clearfix'></view>
</view>

<view class="{{showView?'show':''}} img_box">
  <view class="img_select" bindtap='themeContent' bindtouchstart="touchStart" bindtouchend="touchEnd">
    <image class='img' src="https://www.mqtp8.cn/imgs/putong.png"></image>
    <view class='text'>普通主题</view>
  </view>
  <view class="img_select" bindtap='holiday1' bindtouchstart="touchStart" bindtouchend="touchEnd">
