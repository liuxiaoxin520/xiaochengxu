.header{
  height: 50px;
  width: 100%;
  margin-bottom: 30px;
  background-color: white;
}
.header_left{
  width: 33.3%;
  font-size: 14px;
  text-align: center;
  line-height: 50px;
  float: left;
}
.header_center{
  width: 33.3%;
  font-size: 14px;
  text-align: center;
  line-height: 50px;
  float: left;
}
.header_right{
  width: 33.3%;
  font-size: 14px;
  text-align: center;
  line-height: 50px;
  float: left;
}
.current{
  color: #d24d46;
  border-bottom: 1px solid #d24d46;
}
.show{
  display: none;
}

.img_box{
  padding: 0 5%;
}
.img_select{
  height:180px;
  width: 40%;
  margin: 0 5%;
  margin-bottom: 40px;
  border-radius: 10px;
  float: left;
  background-color: red;
}
.img{
  width: 100%;
  height: 100%;
  border-radius: 10px;
  float: left;
  background-color: blue;
}
.text{
  display: block;
  font-size: 14px;
  text-align: center;
}
