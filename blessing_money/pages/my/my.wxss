.bg_img{
  display: block;
  width: 100%;
  height: 220px;
  position: relative;
  background-repeat: no-repeat;
  background-size: 100%;
}
.header{
  height: 30px;
  width: 150px;
  margin: 0 auto;
  border: 2px solid black;
  border-radius: 20px;
  margin-bottom: 20px;
  background-color: #ffec91;
  box-shadow: -5px -5px 5px #bca63f inset;
}
.myMoney1{
  color: #d24646;
  line-height: 30px;
  text-align: center;
  font-size: 16px;
  font-weight: bold;
}

.show{
  display: none;
}
.photo1{
  display: block;
  margin: 0 auto;
  width: 96rpx;
  height: 96rpx;
  border-radius: 50%;
  margin-bottom: 2px;
  border: 2px solid #ffd75b;
}
.coin{
  display: block;
  margin: 0 auto;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
  margin-bottom: 3px;
  box-shadow: #000 2px 2px 5px;
}
.name1{
  font-size: 14px;
  text-align: center;
  color: white;
}
.container{
  width: 100%;
  margin: 0;
  padding: 0;
  margin-top: 30px;
}
.container_name{
  text-align: center;
  font-size: 14px;
}
.sendMoney_box{
  width: calc(50% - 1px);
  border-right: 1px solid white;
  float: left;
}
.sendCount_box{
  width: calc(50% - 1px);
  border-left: 1px solid white;
  float: left;
}
.bigBox_tip{
  text-align: center;
  font-size: 14px;
  color: #ffec91;
}
.money{
  color: #ffec91;
  font-size: 30px;
  text-align: center;
}
.container_count{
  color: #ffec91;
  font-size: 30px;
  text-align: center;
}

.morePlay{
  text-align: center;
  font-size: 12px;
  margin-bottom: 20px;
}
/* 客服 */
.Contact_service{
  width: 80px;
  position: fixed;
  bottom: 5px;
  left:50%;
  margin-left: -40px;
  font-size: 12px;
  color: #6ac253;
  background-color: white;
}
button::after{
  border:none;
}
.link_voiceBox{
  width: 150rpx;
  margin-left: 20px;
  float: left;
}
.link_voice{
  height: 150rpx;
  width: 150rpx;
  border-radius: 50%;
  box-shadow: #999 5px 5px 5px;
}
.link_voiceName{
  font-size: 12px;
  text-align: center;
}