 <view class="bg_img" style="background-image: url('https://www.mqtp8.cn/imgs/record_img.png')">

<view class="header">
  <view class="myMoney1">余额：￥{{myCount}}</view>
</view>

<view class='bigBox'>
  <view class='sendMoney_box'>
    <image class="photo1" src="{{imgUrl}}" background-size="cover"> </image>
    <view class='name1'>{{name}}</view>
  </view>
  <view class='sendCount_box' bindtap='returnMoney'  bindtouchstart="touchStart" bindtouchend="touchEnd">
    <image class="coin" src="/pages/imgs/coin.png" background-size="cover"> </image>
    <view class='name1'>提现</view>
  </view>
  <view class='clearfix'></view>
</view>
</view>
